<!DOCTYPE html>
<html>
<head>
  <title>Song List</title>
  <style>
    table, th, td { border: 1px solid black; border-collapse: collapse; padding: 8px; }
    th { background-color: #f2f2f2; }
  </style>
</head>
<body>
  <h1>Total Songs: <%= count %></h1>
  <table>
    <tr>
      <th>Song Name</th>
      <th>Film Name</th>
      <th>Music Director</th>
      <th>Singer</th>
      <th>Actor</th>
      <th>Actress</th>
    </tr>
    <% songs.forEach(song => { %>
      <tr>
        <td><%= song.songname %></td>
        <td><%= song.film %></td>
        <td><%= song.music_director %></td>
        <td><%= song.singer %></td>
        <td><%= song.actor || '---' %></td>
        <td><%= song.actress || '---' %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
